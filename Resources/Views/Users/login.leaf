#extend("base")

#export("title") { Login }

#export("body") {
	<form method="post" action="login">
		<label for="username">Username : </label>
		<input type="text" name="username"/>
		<label for="password">Password : </label>
		<input type="password" name="password"/>
		<input type="submit" name="submit"/>
	</form>
}
